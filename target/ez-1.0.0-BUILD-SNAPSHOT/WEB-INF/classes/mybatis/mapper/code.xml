<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nextit.code.service.CodeDao">
	<select id="getCodeListByParent" parameterType="String" resultType="com.nextit.code.vo.CodeVO">
		SELECT cate_name, cate_hrchy, cate_kname
		FROM bo_cate
		WHERE cate_hrchy = #{cateHrchy}
	</select>
</mapper>